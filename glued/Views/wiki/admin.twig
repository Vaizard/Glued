{% extends 'templates/glued.twig' %}

{% block content %}

{{ additional_javascript | raw }}

<div class="title-block">
    <h3 class="title"> Wiki administration </h3>
</div>

<div class="col-md-12">
    <div class="card">
        <div class="card-block">
            <div class="card-title-block">
                <h3 class="title">Existing wikis<a href="{{ path_for('wiki.addwiki') }}" class="pull-right" style="margin: 0 20px;">add new wiki</a></h3>
            </div>
            <section class="example">
                <div class="table-responsive">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Name</th>
                                <th>Url (topic)</th>
                                <th>Articles</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            {{ wiki_output | raw }}
                        </tbody>
                    </table>
                </div>
            </section>
        </div>
    </div>
</div>



{% endblock %}
